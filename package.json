{
	"name": "orhcestrator",
	"private": true,
	"type": "module",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"check": "biome check --write .",
		"dev": "turbo dev",
		"build": "turbo build",
		"check-types": "turbo check-types",
		"deploy": "cd packages/backend && bunx convex deploy",
		"dev:native": "turbo -F native dev",
		"dev:web": "turbo -F web dev",
		"dev:server": "turbo -F @orhcestrator/backend dev",
		"dev:setup": "turbo -F @orhcestrator/backend dev:setup"
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"turbo": "^2.5.8"
	},
	"packageManager": "bun@1.2.18"
}
